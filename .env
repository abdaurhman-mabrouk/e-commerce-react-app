# Base URL for your API (e.g., FakeStoreAPI)
REACT_APP_API_BASE_URL=https://fakestoreapi.com

# API keys (if required)
REACT_APP_API_KEY=your_api_key_here

# Firebase Config
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_storage_bucket
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Auth0 Config
REACT_APP_AUTH0_DOMAIN=your_domain
REACT_APP_AUTH0_CLIENT_ID=your_client_id

# Environment (development, production, test)
NODE_ENV=development

# Debug mode (enable/disable logs)
REACT_APP_DEBUG=true

REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id
REACT_APP_HOTJAR_ID=your_hotjar_id

# Stripe
REACT_APP_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# PayPal
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id

REACT_APP_FEATURE_SEARCH_ENABLED=true
REACT_APP_FEATURE_CART_ENABLED=true

REACT_APP_APP_NAME=My E-Commerce App
REACT_APP_CONTACT_EMAIL=support@example.com
REACT_APP_DEFAULT_CURRENCY=USD

REACT_APP_SENTRY_DSN=your_sentry_dsn
